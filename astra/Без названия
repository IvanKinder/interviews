components:
  schemas:
    Category:
      type: object
      description: сериалайзер для категорий
      properties:
        url:
          type: string
          format: uri
          readOnly: true
        name:
          type: string
          title: Название категории
          maxLength: 16
      required:
      - name
      - url
    CategoryTask:
      type: object
      description: сериалайзер для связи категорий с задачами
      properties:
        url:
          type: string
          format: uri
          readOnly: true
        category:
          type: string
          format: uri
          title: Категория
        task:
          type: string
          format: uri
          title: Задача
      required:
      - category
      - task
      - url
    PaginatedCategoryList:
      type: object
      properties:
        count:
          type: integer
          example: 123
        next:
          type: string
          nullable: true
          format: uri
          example: http://api.example.org/accounts/?page=4
        previous:
          type: string
          nullable: true
          format: uri
          example: http://api.example.org/accounts/?page=2
        results:
          type: array
          items:
            $ref: '#/components/schemas/Category'
    PaginatedCategoryTaskList:
      type: object
      properties:
        count:
          type: integer
          example: 123
        next:
          type: string
          nullable: true
          format: uri
          example: http://api.example.org/accounts/?page=4
        previous:
          type: string
          nullable: true
          format: uri
          example: http://api.example.org/accounts/?page=2
        results:
          type: array
          items:
            $ref: '#/components/schemas/CategoryTask'
    PaginatedTagList:
      type: object
      properties:
        count:
          type: integer
          example: 123
